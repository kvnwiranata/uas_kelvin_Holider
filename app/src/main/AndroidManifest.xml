<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.holider">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_holider"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_holider"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.feedbackFragment"></activity>
        <activity android:name=".ui.main.holidayFragment" />
        <activity
            android:name=".wall_of_holiday"
            android:label="@string/title_activity_wall_of_holiday"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".wall_of_holider" />
    </application>

</manifest>